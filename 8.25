 #define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<string.h>
int main()
{
	char password[20] = { 0 };
	int i = 1;
	int n = 3;
	for (i = 1; i <= n; i++)
	{
		printf("请输入密码>-");
			scanf("%s", password);
			if (strcmp(password, "1366392733") == 0)
			{
				printf("登录成功");
				break;
			}
			else
			{
				printf("密码错误，请重新输入");
			}
	}
	if (i > n)
		printf("三次均输入错误，退出程序。");
	return 0;
}
