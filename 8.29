 #define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
void math__(int ret)
{
	int i = 0;
	int j = 0;
	for (i = 1; i <= ret; i++)
	{
		for (j = 1; j <= i; j++)
		{
			printf(" %d*%d=%-2d ", i, j, i*j);
		}
		printf("\n");
	}
}
int main()
{
	int ret = 0;
	printf("你想求谁的乘法口诀表？\n");
	scanf("%d", &ret);
	math__(ret);
	return 0;
}


